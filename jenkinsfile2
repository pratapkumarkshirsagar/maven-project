 pipeline {
    agent any


    stages {
        stage('SCM Checkout'){
          git 'https://github.com/pratapkumarkshirsagar/maven-project.git'
        }
  }
    
    {
        stage ('Install Stage') {

            steps {
                withMaven(maven : 'EC2MAVEN') {
                    sh 'mvn clean install'
                }
            }
}
}
} 




